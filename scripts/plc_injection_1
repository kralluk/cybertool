import snap7

myplc = snap7.client.Client()

myplc.connect('192.168.50.111',0,2)

print(myplc.get_connected())
print(myplc.get_cpu_state())
print(myplc.db_read(11,0,1))
 

data = myplc.db_read(11,0,1)
program_on = b'\x04'
fake_data = b'\x00'

print(fake_data)
print(type(program_on))

 
myplc.db_write(11,0,fake_data)